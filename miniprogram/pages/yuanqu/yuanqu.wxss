
.box{
	height: 85vh;
	padding: 20rpx;
	display: flex;
	flex-direction: row;
	width: 100%;
}
.info{
	display: flex;
	flex-direction: column;
	width: 48%;
	margin-bottom: 30rpx;
	float: left;
}
.info image{
	width: 330rpx;
	padding: 4rpx;
	border: 2rpx #e6e6e6 solid;
}
.info text{
	font-size: 28rpx;
	text-align: center;
	line-height: 60rpx;
}