.swiper {
  height: 400rpx;
}
.bottom-btnBox {
  position: fixed;
  bottom: 0;
  width: 100%;
  padding: 20rpx 0;
  display: flex;
  justify-content: flex-end;
  box-shadow: 0 -5rpx 30rpx rgba(0, 0, 0, 0.1);
  background-color: white;
}
.bottom-btnBox .btn {
  width: 200rpx;
  margin: 0 10rpx;
  line-height: 50rpx;
  background-color: #da4126;
  color: white;
}
